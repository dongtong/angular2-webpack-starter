{
  "name": "angular2-webpack-starter",
  "version": "0.1.0",
  "description": "Use webpack to build angular 2 application",
  "main": "index.js",
  "scripts": {
    "build": "webpack --inprogress --config webpack.config.prod.js",
    "build:prod": "webpack -p --inprogress",
    "server": "webpack-dev-server --inline --inprogress --config webpack.config.dev.js",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dongtong/angular2-webpack-starter.git"
  },
  "keywords": [
    "Angular2",
    "Webpack",
    "TypeScript",
    "typings"
  ],
  "author": "dongtong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dongtong/angular2-webpack-starter/issues"
  },
  "homepage": "https://github.com/dongtong/angular2-webpack-starter#readme",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.6",
    "@angular/compiler": "^2.0.0-rc.6",
    "@angular/core": "^2.0.0-rc.6",
    "@angular/forms": "^2.0.0-rc.6",
    "@angular/http": "^2.0.0-rc.6",
    "@angular/platform-browser": "^2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.6",
    "@angular/router": "^3.0.0-rc.2",
    "core-js": "^2.4.1",
    "jquery": "^3.1.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.11",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "template-url-loader": "^0.0.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}